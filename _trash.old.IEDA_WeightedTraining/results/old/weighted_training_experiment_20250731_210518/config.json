{
  "experiment": {
    "name": "weighted_training_experiment",
    "seed": 42,
    "device": "cuda",
    "verbose": true
  },
  "data": {
    "data_path": "./KuaiRand",
    "cache_dir": "./KuaiRand/cache",
    "dataset_type": "1K",
    "force_reload_data": false,
    "test_video_ratio": 0.2,
    "p_treatment": 0.5,
    "p_val": 0.2,
    "batch_size": 128,
    "num_workers": 4,
    "shuffle": true,
    "features": {
      "categorical": [
        "user_id",
        "item_id",
        "category"
      ],
      "numerical": [
        "user_age",
        "item_popularity",
        "position"
      ]
    }
  },
  "training": {
    "steps": 10000,
    "eval_steps": 500,
    "lr": 0.001,
    "weight_decay": 1e-05,
    "save_best": true,
    "patience": 5,
    "clip_grad": 1.0,
    "lambda_ctr": 1.0,
    "lambda_playtime": 0.5
  },
  "model": {
    "prediction": {
      "type": "MLP",
      "hidden_dims": [
        128,
        64,
        32
      ],
      "dropout": 0.2,
      "use_batch_norm": true
    },
    "weight": {
      "type": "MLP",
      "hidden_dims": [
        64,
        32
      ],
      "dropout": 0.1,
      "use_batch_norm": true
    },
    "embedding_dim": 16
  },
  "output": {
    "save_dir": "results/",
    "model_dir": "models/",
    "log_level": "INFO"
  }
}